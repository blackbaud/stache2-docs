{
  "name": "stache",
  "mode": "easy",
  "compileMode": "aot",
  "plugins": [
    "@blackbaud/skyux-builder-plugin-stache",
    "@blackbaud/skyux-builder-plugin-auth-email-whitelist"
  ],
  "app": {
    "title": "Blackbaud Stache 2 Documentation"
  },
  "appSettings": {
    "stache": {
      "editButton": {
        "url": "https://github.com/blackbaud/stache2-docs"
      },
      "searchConfig": {
        "is_internal": false
      }
    }
  },
  "auth": false,
  "omnibar": {
    "experimental": true,
    "nav": {
      "services": [
        {
          "title": "Stache 2 Docs",
          "searchUrl": "https://stache-search-query.sky.blackbaud.com/search/stache",
          "items": [
            {
              "title": "Home",
              "route": "/"
            },
            {
              "title": "Learn",
              "items": [
                {
                  "title": "Getting Started",
                  "route": "/learn/get-started"
                },
                {
                  "title": "Basics",
                  "route": "/learn/basics"
                },
                {
                  "title": "Update Stache",
                  "route": "/learn/update-stache"
                },
                {
                  "title": "Plugins",
                  "route": "/learn/plugins"
                },
                {
                  "title": "Blackbaud Procedures",
                  "url": "https://docs.blackbaud.com/stache-internal"
                }
              ]
            },
            {
              "title": "Content",
              "route": "/content"
            },
            {
              "title": "Support",
              "items": [
                {
                  "title": "Contact Us",
                  "route": "/support"
                },
                {
                  "title": "Changelog",
                  "route": "/support/changelog"
                },
                {
                  "title": "Contribute",
                  "route": "/support/contribute"
                }
              ]
            }
          ]
        }
      ]
    }
  }
}


