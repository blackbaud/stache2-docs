{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "stache",
  "mode": "easy",
  "compileMode": "aot",
  "plugins": [
    "@blackbaud/skyux-builder-plugin-stache",
    "@blackbaud/skyux-builder-plugin-code-block"
  ],
  "app": {
    "title": "Blackbaud Stache Documentation",
    "styles": [
      "@skyux/theme/css/sky.css"
    ]
  },
  "appSettings": {
    "stache": {
      "editButton": {
        "url": "https://github.com/blackbaud/stache2-docs"
      },
      "searchConfig": {
        "is_internal": false,
        "site_names": [
          "stache",
          "stache-internal"
        ]
      },
      "footer": {}
    }
  },
  "auth": false,
  "omnibar": {
    "experimental": true,
    "nav": {
      "services": [
        {
          "title": "Stache Docs",
          "searchUrl": "https://stache-search-query.sky.blackbaud.com/suggest/stache",
          "items": [
            {
              "title": "Home",
              "route": "/"
            },
            {
              "title": "Learn",
              "items": [
                {
                  "title": "Overview",
                  "route": "/learn/overview"
                },
                {
                  "title": "Getting started",
                  "route": "/learn/get-started"
                },
                {
                  "title": "Basics",
                  "route": "/learn/basics"
                },
                {
                  "title": "Update Stache",
                  "route": "/learn/update-stache"
                },
                {
                  "title": "Plugins",
                  "route": "/learn/plugins"
                },
                {
                  "title": "Go live",
                  "route": "/learn/golive"
                }
              ]
            },
            {
              "title": "Components",
              "route": "/components"
            },
            {
              "title": "Support",
              "items": [
                {
                  "title": "Contact us",
                  "route": "/support"
                },
                {
                  "title": "Changelog",
                  "route": "/support/changelog"
                },
                {
                  "title": "Feedback",
                  "route": "/support/contribute"
                },
                {
                  "title": "Troubleshooting",
                  "route": "/support/troubleshoot"
                }
              ]
            }
          ]
        }
      ]
    }
  },
  "redirects": {
    "content": "components"
  }
}
